masters namecoinNS {
	95.211.195.245;			# ns myname.bit 
	2001:1af8:4020:a037:1::1000;	# ns ipv6 myname.bit
	113.20.6.2;			# dns.cloudns.com.au
	113.20.8.17;			# dns2.cloudns.com.au
	162.243.56.54;			# ns.coinname.org
	91.213.8.35;			# bit-dns.cryptonit.net
