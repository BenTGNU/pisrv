logging {
    channel "requetes" {
        file "/var/log/named/queries.log" size 10m;
        print-time yes;
        print-category yes;
    };
    category queries { "requetes"; };

    channel "securite" {
        file "/var/log/named/securite.log" size 5m;
        print-category yes;
        print-severity yes;
        print-time yes;
    };
    category security { "securite"; };

    channel "global" {
        file "/var/log/named/global.log" size 5m;
        print-category yes;
        print-severity yes;
        print-time yes;
    };
    category general { "global"; };

    channel "configuration" {
        file "/var/log/named/config.log" size 5m;
        print-category yes;
        print-severity yes;
        print-time yes;
    };
    category config { "configuration"; };
};

